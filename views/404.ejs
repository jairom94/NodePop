<%- include('partials/header.ejs') %>
    <main class="p-404">
        <section class="message">
            <div>
                <figure>
                    <img src="../imgs/404.png" alt="Not found">
                </figure>
                <p>
                    <% if (errors) { %>
                        <ul>
                            <% errors.forEach(e=> { %>
                                <li>
                                    <%= e %>
                                </li>
                                <% }) %>
                        </ul>
                        <% } %>
                </p>
                <div class="go-home">
                    <a href="/">
                        <div>
                            <iconify-icon icon="octicon:home"></iconify-icon>
                        </div>
                        <span>
                            Volver al inicio
                        </span>
                    </a>
                </div>
            </div>
        </section>
    </main>
    <%- include('partials/footer.ejs') %>
        <%- include('scripts/script_home.ejs') %>